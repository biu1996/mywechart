.container {
  width: 100%;
  min-height: 100vh;
  background-color: #f5f5f5;
  -webkit-border-radius: 60rpx 60rpx 0rpx 0rpx;
          border-radius: 60rpx 60rpx 0rpx 0rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  padding: 31rpx;
}
.container .list-none {
  width: 100%;
  min-height: 100vh;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  position: relative;
}
.container .list-none image {
  width: 111rpx;
  height: 120rpx;
}
.container .list-none .none-string {
  color: #a1a4ab;
}
.container .list-content {
  background-color: #ffffff;
  -webkit-border-radius: 60rpx;
          border-radius: 60rpx;
  padding: 35rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  margin-bottom: 40rpx;
}
.container .list-content .intem {
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  position: relative;
}
.container .list-content .intem:nth-child(odd)::after {
  display: none;
}
.container .list-content .intem::after {
  content: '';
  display: block;
  position: absolute;
  top: 72rpx;
  left: 0;
  width: 2rpx;
  height: 80rpx;
  border-right: 2rpx solid #f5f5f5;
}
.container .list-content .intem .intem-image .image {
  width: 311rpx;
  height: 240rpx;
}
.container .list-content .intem .intem-name {
  text-align: center;
  font-size: 30rpx;
  color: #424656;
}
.container .list-content .intem .intem-price {
  text-align: center;
  font-size: 30rpx;
  color: #424656;
  font-weight: bold;
}

