.container {
  width: 100%;
  min-height: 100vh;
  background-color: #f5f5f5;
  -webkit-border-radius: 60rpx 60rpx 0rpx 0rpx;
          border-radius: 60rpx 60rpx 0rpx 0rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  padding: 31rpx;
}
.container .banner {
  width: 690rpx;
  height: 64rpx;
  background-color: #ffffff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  margin-bottom: 30rpx;
  -webkit-border-radius: 60rpx;
          border-radius: 60rpx;
}
.container .banner image {
  width: 24rpx;
  height: 24rpx;
  position: relative;
  top: 22rpx;
  margin-right: 20rpx;
}
.container .banner .bannerinput {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  margin-right: 30rpx;
}
.container .banner .bannerinput .vaninput {
  height: 64rpx;
  -webkit-border-radius: 60rpx;
          border-radius: 60rpx;
}
.container .near {
  background-color: #ffffff;
  -webkit-border-radius: 60rpx;
          border-radius: 60rpx;
  padding: 20rpx 30rpx;
  margin-bottom: 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.container .near .neartext1 {
  font-size: 26rpx;
  color: #424656;
}
.container .near .neartext2 {
  font-size: 26rpx;
  color: #424656;
  font-weight: bold;
}
.container .list-content {
  background-color: #ffffff;
  -webkit-border-radius: 60rpx;
          border-radius: 60rpx;
  padding: 35rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  height: calc(100vh - 100rpx - 60rpx - 100rpx);
}
.container .list-content .nav-left {
  width: 25%;
}
.container .list-content .nav-left .view1 {
  height: calc(100vh - 100rpx - 100rpx - 100rpx);
}
.container .list-content .nav-left .view1 .nav-left-item {
  font-size: 30rpx;
  color: #A1A4AB;
  line-height: 70rpx;
  letter-spacing: 2rpx;
}
.container .list-content .nav-left .view1 .active {
  font-size: 30rpx;
  color: #424656;
  line-height: 70rpx;
  letter-spacing: 2rpx;
  font-weight: bold;
}
.container .list-content .nav-right {
  width: 75%;
}
.container .list-content .nav-right .view2 {
  height: calc(100vh - 100rpx - 100rpx - 100rpx);
}
.container .list-content .nav-right .view2 .view2-flex {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.container .list-content .nav-right .view2 .view2-flex .intem {
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  position: relative;
}
.container .list-content .nav-right .view2 .view2-flex .intem:nth-child(odd)::after {
  display: none;
}
.container .list-content .nav-right .view2 .view2-flex .intem::after {
  content: '';
  display: block;
  position: absolute;
  top: 72rpx;
  left: -20rpx;
  width: 2rpx;
  height: 80rpx;
  border-right: 2rpx solid #f5f5f5;
}
.container .list-content .nav-right .view2 .view2-flex .intem .intem-image .image {
  width: 217rpx;
  height: 169rpx;
}
.container .list-content .nav-right .view2 .view2-flex .intem .intem-name {
  text-align: center;
  font-size: 30rpx;
  color: #424656;
}
.container .list-content .nav-right .view2 .view2-flex .intem .intem-price {
  text-align: center;
  font-size: 30rpx;
  color: #424656;
  font-weight: bold;
}

